<div class="menu-container">
  <div class="card-header col-md-10">
    <h2>
      CURRENT STORAGE
    </h2>
  </div>
  {{#each categories as |category|}}
    <div class="item col-md-5">
      <p>
        {{category.name}}
      </p>
    </div>
  {{/each}}
  {{#each items as |item|}}
    <div class="item col-md-5">
      <p>
        {{item_Name}}.
        {{item.Discription}}
      </p>
    </div>
    <div class="col-md-7">
      <p>
        owner:
        {{item.owner}}
      </p>
    </div>
  {{/each}}
</div>



